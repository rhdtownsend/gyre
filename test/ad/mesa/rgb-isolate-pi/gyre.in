&constants
/

&model
  model_type = 'EVOL'
  file = 'rgb.mesa'
  file_format = 'MESA'
/

&mode
  l = 0
  tag = 'radial'
/

&mode
  l = 1
  tag = 'l1'
/

&mode
  l = 2
  tag = 'l2'
/

&osc
  tag_list = 'radial'
/

&osc
  tag_list = 'l1,l2'
  alpha_pi = 0.
  x_atm = 0.9
/

&rot
/

&num
  diff_scheme = 'COLLOC_GL4'
/

&scan
  grid_type = 'LINEAR'
  freq_min = 1
  freq_max = 90
  freq_min_units = 'UHZ'
  freq_max_units = 'UHZ'
  n_freq = 50
/

&grid
  w_osc = 10
  w_exp = 2
  w_ctr = 100
  tag_list = 'radial'
/

&grid
  w_osc = 10
  w_exp = 2
  w_ctr = 100
  tag_list = 'l1,l2'
/

&ad_output
  summary_file = 'summary.h5'
  summary_item_list = 'id,l,n_pg,n_p,n_g,omega'
/

&nad_output
/
