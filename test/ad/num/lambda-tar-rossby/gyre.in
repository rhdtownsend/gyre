  freq_max_units = 'ROSSBY_O'
  freq_frame = 'COROT_O'
  n_freq = 50
/

&grid
/

&ad_output
  summary_file = 'summary.h5'
  summary_item_list = 'id,l,m,n_pg,n_p,n_g,omega'
/

&nad_output
