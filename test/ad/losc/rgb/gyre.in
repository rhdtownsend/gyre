&constants
  G_GRAVITY = 6.674280000000001E-008
/

&model
  model_type = 'EVOL'
  file = '1.5M_6022_n61.profile.FGONG.oscmod-better'
  file_format = 'LOSC'
/

&mode
  l = 1
/

&osc
/

&rot
/

&num
  diff_scheme = 'COLLOC_GL6'
/

&scan
  grid_type = 'INVERSE'
  freq_min = 110
  freq_max = 112
  freq_min_units = 'UHZ'
  freq_max_units = 'UHZ'
  n_freq = 20
/

&grid
  w_osc = 10.
  w_exp = 2.
  w_ctr = 10.
/

&ad_output
  summary_file = 'summary.h5'
  summary_item_list = 'id,l,n_pg,n_p,n_g,freq'
/

&nad_output
/

