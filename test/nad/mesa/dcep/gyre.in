&constants
/

&model
  model_type = 'EVOL'
  file = 'dcep.mesa'
  file_format = 'MESA'
/

&mode
  l = 0
/

&osc
  inner_bound = 'ZERO_R'
  nonadiabatic = .TRUE.
/

&rot
/

&num
  diff_scheme = 'MAGNUS_GL2'
  restrict_roots = .FALSE.
/

&scan
  grid_type = 'LINEAR'
  freq_min = 1.0
  freq_max = 10.0
  n_freq = 50
/

&grid
  x_i = 0.0015
  w_osc = 10.
  w_exp = 2.
/

&ad_output
/

&nad_output
  summary_file = 'summary.h5'
  summary_item_list = 'id,l,n_pg,n_p,n_g,omega'
/
